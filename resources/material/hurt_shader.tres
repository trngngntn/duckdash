[gd_resource type="Shader" format=2]

[resource]
code = "shader_type canvas_item;
uniform bool enable = false;

void fragment(){
	vec4 color = texture(TEXTURE, UV);
	if(enable && color.a == 1f){
		COLOR = vec4(mix(color.rgb, vec3(1f,1f,1f), 0.85 /** abs(sin(TIME * 15f))*/), 1);
	} else {
		COLOR = color
	}
}"
